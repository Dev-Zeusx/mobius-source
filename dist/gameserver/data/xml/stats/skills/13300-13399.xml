			мощность Крит. Атк., Точность/Скор. Атк./Скор. Маг./скорость восстановления MP. -->
		<!-- GOD Skill -->
		<!-- Boss Jewel Skill -->
		<set name="magicLvl" val="85" />
		<set name="operateType" val="OP_PASSIVE" />
		<set name="skillType" val="BUFF" />
		<set name="target" val="TARGET_SELF" />
		<for>
			<!-- Все статы от балды -->
			<sub order="0x40" stat="knockBackResist" val="10" />
			<sub order="0x40" stat="knockDownResist" val="10" />
			<add order="0x40" stat="magicResist" val="2" />
			<add order="0x40" stat="physResist" val="2" />
			<add order="0x40" stat="poisonPower" val="50" />
			<sub order="0x40" stat="poisonResist" val="50" />
			<!-- Values is guessed -->
			<mul order="0x30" stat="cAtk" val="1.15" />
			<mul order="0x30" stat="mAtkSpd" val="1.04" />
			<mul order="0x30" stat="pAtkSpd" val="1.04" />
			<add order="0x40" stat="defenceWind" val="20" />
			<add order="0x40" stat="defenceWater" val="20" />
			<add order="0x40" stat="mDef" val="112" />
			<add order="0x40" stat="pDef" val="112" />
			<mul order="0x30" stat="regMp" val="1.1"/>
