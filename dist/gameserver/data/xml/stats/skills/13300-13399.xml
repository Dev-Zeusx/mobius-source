	<skill id="13313" levels="1" name="Благословенная Серьга Антараса">
		<!--
			Увеличивает Защиту/Маг. Защиту; защиту от земли/света; сопротивление кровопусканию/шоковым/ментальным атакам; 
			добавляет бонус кровопусканию/шоковым/ментальным атакам; усиливает эффективность магии, восстанавливающей HP;
			уменьшает расход MP на умения; 
			с определенной вероятностью восстанавливает HP за счет урона, нанесенного врагу. ВЫН +3, ДУХ +3.	
		-->
		<set name="icon" val="icon.accessory_necklace_of_valakas_i00" />
		<set name="magicLevel" val="90" />
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<add order="0x40" stat="bleedResist" val="50" />
			<add order="0x40" stat="bleedPower" val="50" />
			<mul order="0x30" stat="hpEff" val="1.40" />
			<add order="0x40" stat="mentalResist" val="40" />
			<add order="0x40" stat="mentalPower" val="40" />
			<add order="0x40" stat="stunResist" val="40" />
			<add order="0x40" stat="stunPower" val="40" />			
			<add order="0x40" stat="absorbDam" val="4" />
			<mul order="0x30" stat="mpConsum" val="0.90" />			
			<add order="0x40" stat="defenceEarth" val="30" />
			<add order="0x40" stat="defenceHoly" val="15" />			
			<add order="0x10" stat="CON" val="3"/>			
			<add order="0x10" stat="MEN" val="3"/>			
		</for>
	</skill>
	
	<skill id="13315" levels="1" name="Благословенное Ожерелье Валакаса">
